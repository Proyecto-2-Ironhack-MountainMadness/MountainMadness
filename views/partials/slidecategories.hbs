<!-- Carousel wrapper -->

<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
          {{#each tracks as |track i|}}
          {{#if i}}
          <div class="carousel-item">
            <a href="/tracks/{{track._id}}">
              <h4 class="title-carousel" >{{track.title}}</h4>
              <img class="image-div d-block w-100"
                src="{{#if track.image}}{{track.image}}{{else}}https://images.unsplash.com/photo-1599493458967-030d9a03c2c2?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=634&q=80{{/if}}"
                alt="First slide">
            </a>
          </div>
          {{else}}
          <div class="carousel-item active">
            <a href="/tracks/{{track._id}}">
              <h4 class="title-carousel">{{track.title}}</h4>
              <img class="image-div d-block w-100"
                src="{{#if track.image}}{{track.image}}{{else}}https://images.unsplash.com/photo-1599493458967-030d9a03c2c2?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=634&q=80{{/if}}"
                alt="First slide">
            </a>
          </div>
          {{/if}}
          {{/each}}
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>

  </div>

</div>