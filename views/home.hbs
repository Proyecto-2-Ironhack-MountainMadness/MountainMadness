<div id="video-container-home">
  <video class="video-home" width="100%" playsinline="playsinline" autoplay="autoplay" muted="muted" loop="loop">
    <source src="/images/videoHome.mp4" type="video/mp4">
  </video>
  <div id="tracks-home-link">
    <a class="container" href="/tracks">
    <div class="typewriter">
      <p class="line-1 anim-typewriter">Descubre tu próxima aventura aquí</p>
      </div>
    </a>
  </div>
</div>

